{
    "name": "xactsystems/check-route-security",
    "description": "A Symfony command to check route security in controllers.",
    "type": "symfony-bundle",
    "license": "MIT",
    "version": "1.0.0",
    "require": {
        "php": ">=7.4",
        "symfony/config": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dependency-injection": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/http-kernel": "4.4.*",
        "symfony/routing": "4.4.*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-symfony": "^1.3",
        "slevomat/coding-standard": "^8.14",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/phpunit-bridge": "^6.3"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Xact\\CheckRouteSecurity\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xact\\CheckRouteSecurity\\Tests\\": "tests/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    },
    "scripts": {
        "auto-scripts": {

        }
    }
}
